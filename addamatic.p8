--logic

--mouse x,y and left click
function upd_mouse()
	pmx,pmy,pmb=mx,my,mb

 mx=stat(32)
	my=stat(33)
	mb=stat(34)&1
	click=(mb==pmb+1)
end

--checks if mouse over button
function upd_btn_hover()
	for b in all(buttons) do
		local x,y = btn_xy(b)
		if dist(x,y,mx,my)<=29 then
			b.h = true
		else
			b.h = false
		end
	end
end

--check if any button was clicked on
function upd_btn_click()
	btn_c=btn_closest()
	if btn_c.h then
		perform_btn_click(btn_c)
	else
		btn_c=nil
	end
end

--add the value of the button to the sum
function perform_btn_click(b)
	sum += b.y*bspot(b.x)
	sum%=tonum("1000000000",2)

	set_dial_tgt()
end

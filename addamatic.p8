--the add-a-matic?
--bY kULPAS
ver="V0.09"

function _init()
	--metainit
	poke(0x5f2d, 1)
	log("start log",true)
	debug={}

	--h means hover
	--c means clicked

	--prev frame x,y,l-btn and current x,y,l-btn
	pmx,pxy,pmb,mb,mx,my=0,0,0,0,0,0
	btn_c=nil --pointer to clicked on btn

	bar_y=body_y+2
	bar_h=false
	bar_c=false

	dial_pos={} --where the dials are now
	dial_tgt={}	--where they should be to display corrent number

	sum=0 --the actual number we're counting!


	--init target & set position to target
	set_dial_tgt() 
	for i=0,8 do
		dial_pos[i]=dial_tgt[i]
	end

	--create buttons
	buttons={}
	for i=0,39 do
		--y 1indexed to double as btn value
		b={x=i%8,y=i\8+1}
		add(buttons,b)
	end
end

function _update()
	--mouse pos
	upd_mouse()

	--bar movement
	if bar_c then
		bar_y += my-pmy
		bar_y=mid(54,bar_y,103)
	else
		bar_y-=3
		bar_y=max(bar_y,54)
	end
	
	if bar_y>101 then
		sum=0
		set_dial_tgt(true)
	end

	--hover on buttons and bar
	upd_btn_hover()
	bar_h=mid(9,14,mx)==mx and mid(bar_y,bar_y+11,my)==my 
	
	--clicking
	if click then
		upd_btn_click()
		if bar_h then
			bar_c = true
		end
	end

	--button release
	if mb==0 or (btn_c and not btn_c.h) then
		btn_c=nil
	end
	if mb==0 or (bar_c and not bar_h) then
		bar_c=false
	end

	--debug
	debug[1]=mx.." "..my
	debug[2]=bar_h
	debug[3]=bar_c
	debug[4]=tostr(sum,2)
end

function _draw()
	cls(5)

	set_dial_pos()
	draw_dials()

	draw_outline()
	draw_bar(body_x+3,bar_y)

	--draw buttons
	for b in all(buttons) do
		draw_button(b)
	end

	--draw mouse
	palt(0x4000)
	spr(15,mx-3,my,1,2)
	palt()

	print_debug()
end

--graphics

logo_w,logo_h = 128,28
body_x,body_y,body_w,body_h=6,51,116,65
disp_x,disp_y = 4,32

--draw all the placeholders
function draw_outline()
	--logo
	rect2(0,0,logo_w,logo_h,0)
	rect2(2,2,logo_w-4,logo_h-4,10)

	--main body
	rect2(body_x-1,body_y-1,body_w+2,body_h+2,0)
	rect2(body_x,body_y,body_w,body_h,15)
	--reset bar line
	rect2(body_x+5,body_y+8,2,body_h-16,0)


	--footer
	rect2(0,121,128,7,6)
	print(ver,108,122,5)
end

--draws a button from the button obj
function draw_button(b)
	local x,y=btn_xy(b)
	local c=btn_color(b)
	if(b==btn_c) y+=1
	circfill(x,y,5,c)
	print(b.y,x-1,y-2,0)
end

--draws a single number dial "strip"
function draw_dial(x,y)
	rect2(x,y,5,67,7)
	for i=0,10 do
		print(i%10,x+1,y+1+(i*6),0)
	end
end

--draws all the number "strips" in correct positions
function draw_dials()
	clip(disp_x-1,disp_y-1,122,15)
	for i=0,8 do
		draw_dial(disp_x+109-i*13,dial_pos[i])
	end

	--frame
	rect2(disp_x,disp_y,120,3,6)
	rect2(disp_x,disp_y+10,120,3,6)
	for i=0,7 do
		rect2(disp_x+10+i*13,disp_y+3,8,7,6)
		if(i<2)rect2(disp_x-1+i*115,disp_y+3,6,7,6)
	end
	for i=0,2 do
		rect2(disp_x+13+i*39,disp_y+7,2,2,8)
	end

	rect(disp_x-1,disp_y-1,disp_x+120,disp_y+13,0)
	clip()
end

--draws the reset bar
function draw_bar(x,y)
	rect2(x,y,6,12,0)
	rect2(x+1,y+1,4,10,8)
end

--sets the target position of all dials
function set_dial_tgt(backward)
	local s=sum
	for i=0,8 do
		local a=s%(10>>16)
		a<<=16
		s/=10

		--bad wrap fix part 1
		--sometimes sets target to 2nd zero
		if(not backward and a==0)a=10
		
		dial_tgt[i]=disp_y+3-a*6
	end
end

--moves all dials in target direction
function set_dial_pos()
	local top,bot=disp_y+3,disp_y-57 --dial pos of zeroNO1 & zeroNO2
	
	for i=0,8 do
		local spd,tgt,pos=1.5,dial_tgt[i],dial_pos[i]
		
		--bad wrap fix part 3
		if abs(tgt-pos)<=spd then
			dial_pos[i]=tgt
		elseif tgt==top then
			--go up if going to zero 1
			dial_pos[i]+=spd
		else
			--go down in any other case
			dial_pos[i]-=spd
			if dial_pos[i]<=bot then 
				dial_pos[i]=top
			end --bruh use locals
		end
		
		
		
		--bad wrap fix part 2
		--teleports to zero 1 when at zero 2
		--also overrides zero 1 to zero 2 jumps
		if tgt==bot and (pos==top or pos==bot) then
			dial_tgt[i],dial_pos[i]=top,top
		end
	end
end

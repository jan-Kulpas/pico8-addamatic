__lua__
--the add-a-matic?
--bY kULPAS
ver="V0.04"

function _init()
	--metainit
	poke(0x5f2d, 1)
	log("start log",true)
	debug={}
	
	pmb,mb=0,0
	btn_c=nil
	
	sum=0
	

	--create buttons
	buttons={}
	for i=0,39 do
		--y 1indexed to double as btn value
		b={x=i%8,y=i\8+1}
		add(buttons,b)
	end
end

function _update()
	--mouse pos
	upd_mouse()
	

	--clicking on buttons	
	upd_btn_hover()
	
	if click then
		btn_c=btn_closest()
		if btn_c.hover then
			perform_btn_click(btn_c)
		else
			btn_c=nil
		end
	end
	
	--button release
	if (btn_c and not btn_c.hover) or mb==0 then
		btn_c=nil
	end
	
	
 
	
	--debug
	debug[1]=mx
	debug[2]=my
	local a,b=btn_xy(buttons[1])
	debug[3]=dist(a,b,mx,my)
	debug[4]=tostr(sum,2)
end

function _draw()
	cls(5)
	
	draw_outline()	
 --px_grid()
	
	--draw buttons
	for b in all(buttons) do
		draw_button(b)
	end 
	
	--draw mouse
	palt(0x4000)
	spr(15,mx-3,my,1,2)
	palt()
	
	print_debug()
end

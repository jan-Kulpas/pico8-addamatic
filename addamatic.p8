--helpers

--rectfill with width height
function rect2(x,y,w,h,c)
	rectfill(x,y,x+w-1,y+h-1,c)
end

--return button center based on id
function btn_xy(b)
	return (body_x+body_w-7)-b.x*13, body_y+body_h+6-b.y*13
end

--return color of a button
--using a bitmap:
--2*position+hover
function btn_color(b)
	local colors={[0]=11,3,10,9}
	local p=(b.x<2 or b.x==5) and 2 or 0
	return colors[tonum(b.h)+p]
end

--returns closest button
function btn_closest()
	local bd,best,d = 32767,nil,0
	for b in all(buttons) do
		local x,y=btn_xy(b)
		d=dist(x,y,mx,my)
		if d<bd then
			bd=d
			best=b
		end
	end
	return best
end

--8x8 grid for pixel counting
function px_grid()
	for x=0,31 do
		for y=0,31 do
			rect(x*8,y*8,x*8+7,y*8+7,(x+y)%2==0 and 2 or 13)
		end
	end
end

--bit-shifted power of ten
function bspot(x)
	local r=1>>16
	for i=0,x-1 do
		r*=10
	end
	return r
end

--literally dist
function dist(x1,y1,x2,y2)
	local dx,dy = x2-x1, y2-y1
	return dx*dx+dy*dy
end

--logging
function log(t,over)
	printh(t,"log",over or false)
end

--prints debug table in top left
function print_debug()
	local y=3
	for txt in all(debug) do
		print(txt,3,y,8)
		y+=6
	end
end

